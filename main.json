{
  "AWSTemplateFormatVersion" : "2010-09-09",
  "Description" : "Deploy team.madetech.com",
  "Resources" : {
    "TeamMadeTech" : {
      "Type" : "AWS::S3::Bucket",
      "Properties" : {
        "AccessControl" : "PublicRead"
      }
    }
  },
  "Outputs" : {
    "WebsiteURL": {
      "Value" : { "Fn::GetAtt" : [ "TeamMadeTech", "WebsiteURL" ] },
      "Description" : "URL for s3 bucket"
    }
  }
}
